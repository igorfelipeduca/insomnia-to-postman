#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/duca/indie/insomnia-to-postman/node_modules/.pnpm/insomnia-to-postman@1.0.5_@types+node@22.14.1_typescript@5.8.3/node_modules/insomnia-to-postman/dist/node_modules:/Users/duca/indie/insomnia-to-postman/node_modules/.pnpm/insomnia-to-postman@1.0.5_@types+node@22.14.1_typescript@5.8.3/node_modules/insomnia-to-postman/node_modules:/Users/duca/indie/insomnia-to-postman/node_modules/.pnpm/insomnia-to-postman@1.0.5_@types+node@22.14.1_typescript@5.8.3/node_modules:/Users/duca/indie/insomnia-to-postman/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/duca/indie/insomnia-to-postman/node_modules/.pnpm/insomnia-to-postman@1.0.5_@types+node@22.14.1_typescript@5.8.3/node_modules/insomnia-to-postman/dist/node_modules:/Users/duca/indie/insomnia-to-postman/node_modules/.pnpm/insomnia-to-postman@1.0.5_@types+node@22.14.1_typescript@5.8.3/node_modules/insomnia-to-postman/node_modules:/Users/duca/indie/insomnia-to-postman/node_modules/.pnpm/insomnia-to-postman@1.0.5_@types+node@22.14.1_typescript@5.8.3/node_modules:/Users/duca/indie/insomnia-to-postman/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../insomnia-to-postman/dist/index.js" "$@"
else
  exec node  "$basedir/../insomnia-to-postman/dist/index.js" "$@"
fi
