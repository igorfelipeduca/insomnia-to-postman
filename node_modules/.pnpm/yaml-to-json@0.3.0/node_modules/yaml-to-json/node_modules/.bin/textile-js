#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/duca/indie/insomnia-to-postman/node_modules/.pnpm/textile-js@0.1.11/node_modules/textile-js/bin/node_modules:/Users/duca/indie/insomnia-to-postman/node_modules/.pnpm/textile-js@0.1.11/node_modules/textile-js/node_modules:/Users/duca/indie/insomnia-to-postman/node_modules/.pnpm/textile-js@0.1.11/node_modules:/Users/duca/indie/insomnia-to-postman/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/duca/indie/insomnia-to-postman/node_modules/.pnpm/textile-js@0.1.11/node_modules/textile-js/bin/node_modules:/Users/duca/indie/insomnia-to-postman/node_modules/.pnpm/textile-js@0.1.11/node_modules/textile-js/node_modules:/Users/duca/indie/insomnia-to-postman/node_modules/.pnpm/textile-js@0.1.11/node_modules:/Users/duca/indie/insomnia-to-postman/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../textile-js@0.1.11/node_modules/textile-js/bin/textile" "$@"
else
  exec node  "$basedir/../../../../../textile-js@0.1.11/node_modules/textile-js/bin/textile" "$@"
fi
